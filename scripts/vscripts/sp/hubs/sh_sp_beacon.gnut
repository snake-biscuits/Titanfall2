			RegisterDialogue( "RandomGrunt_Banter_1a", 				"diag_sp_Spoke0_BE331_01_01_mcor_grunt1", 	PRIORITY_NO_QUEUE ) 		// Who is this loudspeaker talking to? It's not like these tin-cans speak english.
			RegisterDialogue( "RandomGrunt_Banter_1b", 				"diag_sp_Spoke0_BE331_02_01_mcor_grunt2", 	PRIORITY_NO_QUEUE ) 		// Maybe they can...
			RegisterDialogue( "RandomGrunt_Banter_2", 				"diag_sp_Spoke0_BE331_03_01_mcor_grunt3", 	PRIORITY_NO_QUEUE ) 		// Pilot, more Stalkers are expected... We'll hold them off but don't be gone too long.
			RegisterDialogue( "RandomGrunt_Banter_3", 				"diag_sp_Spoke0_BE331_04_01_mcor_grunt4", 	PRIORITY_NO_QUEUE ) 		// We're getting low on ammo... and soldiers.
			RegisterDialogue( "RandomGrunt_Banter_4", 				"diag_sp_Spoke0_BE331_05_01_mcor_grunt5", 	PRIORITY_NO_QUEUE ) 		// We're doing our best down here.
			RegisterDialogue( "RandomGrunt_Banter_5", 				"diag_sp_Spoke0_BE331_06_01_mcor_grunt6", 	PRIORITY_NO_QUEUE ) 		// It's great to have a Pilot with us again. Looks like we'll have a chance of getting out alive after all.
			RegisterDialogue( "RandomGrunt_Banter_6", 				"diag_sp_Spoke0_BE331_07_01_mcor_grunt3", 	PRIORITY_NO_QUEUE ) 		// I wonder how deep this facility goes.
			RegisterDialogue( "RandomGrunt_Banter_7", 				"diag_sp_Spoke0_BE331_09_01_mcor_grunt7", 	PRIORITY_NO_QUEUE ) 		// Careful people. These Stalkers can come out of the walls...Stay alert.
			RegisterDialogue( "RandomGrunt_Banter_8a", 				"diag_sp_Spoke0_BE101_05_01_mcor_grunt3", 	PRIORITY_NO_QUEUE ) 		// We should just seal this place off and be done with it...
			RegisterDialogue( "RandomGrunt_Banter_8b", 				"diag_sp_Spoke0_BE331_10_01_mcor_grunt8", 	PRIORITY_NO_QUEUE ) 		// That's your answer to everything, Barry.
			RegisterDialogue( "RandomGrunt_Banter_9", 				"diag_sp_Spoke0_BE331_11_01_mcor_grunt9", 	PRIORITY_NO_QUEUE ) 		// This place is run by machines. It's not like there's an elevator around here. How's he gonna get through this place?
			RegisterDialogue( "RandomGrunt_Banter_10", 				"diag_sp_Spoke0_BE101_07_01_mcor_grunt2", 	PRIORITY_NO_QUEUE ) 		// It looks like they can turtle up to protect their heads. That's why they were so hard to kill...
			RegisterDialogue( "RandomGrunt_Banter_11", 				"diag_sp_Spoke0_BE331_12_01_mcor_grunt12", 	PRIORITY_NO_QUEUE ) 		// Wish I could tell you what's down there, but whatever it is, we're hoping you can handle it, Pilot.

			RegisterDialogue( "RandomGrunt_LastPilotWentThatWay", 	"diag_sp_Spoke0_BE101_08_01_mcor_grunt2", 	PRIORITY_NO_QUEUE ) 		// The last Pilot in our unit went that way a few hours ago, but he never came back. Be careful.

