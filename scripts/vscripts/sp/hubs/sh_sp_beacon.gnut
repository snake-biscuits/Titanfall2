global function ShBeaconCommonInit

void function ShBeaconCommonInit()
{
	switch ( GetMapName() )
	{
		case "sp_beacon":

			// carry over from spoke 0
			PrecacheWeapon( "mp_weapon_softball" )
			PrecacheWeapon( "mp_weapon_deployable_cover" )

			RegisterOnscreenHint( "DisembarkHint", "#HUD_TITAN_DISEMBARK" )
			RegisterOnscreenHint( "ArcToolHint", "#BEACON_SWITCH_TO_ARC_TOOL_HINT" )

			//########################
			// 		  Intro
			//########################

			RegisterDialogue( "BT_WeShouldGetMoving", 			"diag_sp_hubApproach_BE101_02_01_mcor_bt", 			PRIORITY_NORMAL )		// We should get moving Pilot. (nag)
			RegisterDialogue( "BT_TheresTheBeacon", 			"diag_sp_intro_BE301_06_01_mcor_bt", 				PRIORITY_NO_QUEUE )		// There's the beacon. Making contact with a rescue ship should be our top priority.
			RegisterDialogue( "BT_ItMayNeedRepairs", 			"diag_sp_intro_BE301_07_01_mcor_bt", 				PRIORITY_NORMAL )		// Scans detect minimal power functionality. It may need repairs.
			RegisterDialogue( "BT_MultipleContacts", 			"diag_sp_intro_BE301_08_01_mcor_bt", 				PRIORITY_NORMAL )		// Careful. I am picking up multiple contacts, range 300 meters.
			RegisterDialogue( "BT_HostilesConfirmed", 			"diag_sp_fogFields_BE311_02_01_mcor_bt", 			PRIORITY_NO_QUEUE )		// Hostiles confirmed: The Stalkers appear to be programmed to attack the control room.
			RegisterDialogue( "BT_FogIsDeadlyToOrganics", 		"diag_sp_hubApproach_BE102_01a_01_mcor_bt", 		PRIORITY_NORMAL )		// Pilot, I recommend you get in the cockpit - that fog is deadly to organic life. (nag)

			//#####################
			// Control Room Battle
			//#####################

			// First contact from Militia in the control room
			RegisterDialogue( "BT_DetectingMilitiaForces",		"diag_sp_fogFields_BE311_03_01_mcor_bt", 			PRIORITY_NORMAL )	// Pilot, I am detecting Militia forces inside that beacon control room. They are signalling distress. Patching in.
			RegisterDialogue( "BT_CrackleCrackle",				"diag_sp_hubApproach_BE102_03_01_mcor_bt", 			PRIORITY_NORMAL )	// <...crackle crackle unintelligible...>
			RegisterRadioDialogue( "Mike_WeCanUseSomeHelp",			"diag_sp_fogFields_BE311_04_01_mcor_gcaptain", 		PRIORITY_NORMAL, "#CPT_COLE", TEAM_MILITIA )	// Hey, you out there! Pilot! Are you reading us? We could use some help over here!
			RegisterRadioDialogue( "Mike_PleaseAssist",				"diag_sp_fogFields_BE311_05_01_mcor_gcaptain", 		PRIORITY_NORMAL, "#CPT_COLE", TEAM_MILITIA )	// We're from the 3rd Militia Grenadiers! We're getting overrun by Stalkers! Please assist!
			RegisterDialogue( "BT_WeShouldHelp",				"diag_sp_fogFields_BE311_08_01_mcor_bt", 			PRIORITY_NORMAL )	// I suggest we help eliminate these Stalkers, and then make direct contact with the Militia Riflemen.

			// Nags if the player isn't helping the fight
			RegisterRadioDialogue( "Mike_Nag_NeedYourAssistance",	"diag_sp_fogFields_BE311_06_01_mcor_gcaptain", 		PRIORITY_NORMAL, "#CPT_COLE", TEAM_MILITIA )	// We can't hold them off for much longer. We need your assistance! Over.
			RegisterRadioDialogue( "Mike_Nag_BadShape",				"diag_sp_fogFields_BE311_07_01_mcor_gcaptain", 		PRIORITY_NORMAL, "#CPT_COLE", TEAM_MILITIA )	// We're in bad shape! Please assist! Over.

			// During combat, conditional
			RegisterDialogue( "BT_GoodThinkingRunThemOver",		"diag_sp_fogFields_BE311_10_01_mcor_bt", 			PRIORITY_NORMAL )	// Interesting tactic, Pilot. The 10,000 pound weight of my chassis caneliminate Stalkers more efficiently.
			RegisterDialogue( "BT_YouCanRunThemOver",			"diag_sp_fogFields_BE311_11_01_mcor_bt", 			PRIORITY_NORMAL )	// Tactical option: run them over. My 10,000 pound chassis can eliminate smaller enemies.

			// All stalkers are dead
			RegisterDialogue( "BT_ThreatNeutralized", 			"diag_sp_fogFields_BE311_12_01_mcor_bt", 			PRIORITY_HIGH ) 	// Threat neutralized. Well done.This victory has raised our combat efficiency rating.
			RegisterDialogue( "BT_AllThreatsEliminated",		"diag_sp_fogFields_BE311_13_01_mcor_bt", 			PRIORITY_HIGH )	// Guardian One to Militia forces - all threats have been eliminated.
			RegisterRadioDialogue( "Mike_YouSavedOurAsses",			"diag_sp_fogFields_BE311_14_01_mcor_gcaptain", 		PRIORITY_HIGH, "#CPT_COLE", TEAM_MILITIA )	// You saved our asses. Those Stalkers just kept on coming -- we may not have lasted much longer.
			RegisterRadioDialogue( "Mike_ComeOnIn",					"diag_sp_hubApproach_BE102_14_01_mcor_grunt1", 		PRIORITY_HIGH, "#CPT_COLE", TEAM_MILITIA )	// We're opening the blast doors. Come on in.

			// Nag if player doesn't enter control room
			RegisterDialogue( "BT_Nag_GoIntoControlRoom",		"diag_sp_fogFields_BE311_15_01_mcor_bt", 			PRIORITY_HIGH )	// We can attempt beacon activation from the control room. I recommend we go there.

			// After control room doors are open
			RegisterRadioDialogue( "Mike_BlastDoorsAreOpen",			"diag_sp_fogFields_BE311_16_01_mcor_gcaptain", 		PRIORITY_HIGH, "#CPT_COLE", TEAM_MILITIA )	// The blast doors are open, Pilot. Come on in. We're trying to send a signal but the beacon was hit hard. We're having some trouble bringing it back online.

			//#########################
			// Inside the Control Room
			//#########################

			RegisterDialogue( "Grunt1_BringEmUp", 				"diag_sp_controlBunker_BE321_01_01_mcor_gcaptain", 	PRIORITY_NO_QUEUE )	// The Pilot and Titan are on the lift. McCord! Bring 'em up. Let's go!
			RegisterDialogue( "Grunt2_RogerThat", 				"diag_sp_controlBunker_BE321_02_01_mcor_grunt1", 	PRIORITY_NO_QUEUE )	// Right away, sir!
			RegisterDialogue( "BT_DisembarkNag", 				"diag_sp_controlRoom_BE901_01_01_mcor_bt",			PRIORITY_NORMAL ) 	// Pilot, I advise you disembark to engage the Militia Riflemen directly.
			RegisterDialogue( "BT_RequestBeaconStatus", 		"diag_sp_controlBunker_BE321_07_01_mcor_bt", 		PRIORITY_NO_QUEUE )	// Captain, request beacon status.
			RegisterDialogue( "Mike_PowerHasShutDown", 			"diag_sp_controlBunker_BE321_08_01_mcor_gcaptain", 	PRIORITY_NO_QUEUE )	// The power has shut down. We've lost all control of the beacon. We need something like this to jump start the system, but this one's destroyed.
			RegisterDialogue( "BT_DesignationArcTool", 			"diag_sp_controlBunker_BE321_09_01_mcor_bt", 		PRIORITY_NO_QUEUE )	// Designation: Arc Tool. Function: Generates power up to 5 Megajoules. Developed by IMC at Gridiron Station 207. Dr. Colby Marvin--
			RegisterDialogue( "Mikes_WeKnowWhatItIs", 			"diag_sp_controlBunker_BE321_10_01_mcor_gcaptain", 	PRIORITY_NO_QUEUE )	// Yes. Yes...We know what it is.
			RegisterDialogue( "BT_IsItSalvageable", 			"diag_sp_controlBunker_BE321_11_01_mcor_bt", 		PRIORITY_NO_QUEUE )	// Is it salvageable?
			RegisterDialogue( "Mike_UnfortunatelyNot", 			"diag_sp_controlBunker_BE321_12_01_mcor_gcaptain", 	PRIORITY_NO_QUEUE )	// Unfortunately not. Sensors indicate another one in the substation, but it's nearly impossible to get down there. This place just ain't built for humans.
			RegisterDialogue( "BT_UseCautionNavigating", 		"diag_sp_recExtra_BE701_01_01_mcor_bt", 			PRIORITY_NO_QUEUE )	// Understood. Pilot Cooper, I advise caution while navigating this facility's substation. You must recover the Arc Tool.
			RegisterDialogue( "Mike_CooperIsThatYou", 			"diag_sp_recExtra_BE701_03_01_mcor_gcaptain", 		PRIORITY_NO_QUEUE )	// Cooper? Is that you? Looks like you got a story to tell. Save it for when we get off this rock.

			RegisterDialogue( "BT_GoodLuckPilot", 				"diag_sp_controlBunker_BE321_15_01_mcor_bt", 		PRIORITY_NO_QUEUE )	// Good luck, Pilot.
			RegisterDialogue( "Grunt_GoodLuckSir", 				"diag_sp_controlBunker_BE321_16_01_mcor_grunt3",	PRIORITY_NORMAL )	// Good luck sir, hope you make it back. We've lost too many down there.
			RegisterDialogue( "BT_Nag_ThatDoorOverThere", 		"diag_sp_controlBunker_BE321_17_01_mcor_bt",		PRIORITY_NORMAL )	// Pilot, the beacon power substation is through that giant door marked "Power Grid"

			// Grunt dialogue after mission dialogue (solos)
			RegisterDialogue( "Grunt_4_StalkersKeptComing", 	"diag_sp_controlBunker_BE321_18_01_mcor_grunt4",	PRIORITY_NORMAL )	// Those Stalkers just kept coming. It's a good thing that Pilot and that Titan showed up...
			RegisterDialogue( "Grunt_5_PickingThemOff", 		"diag_sp_controlBunker_BE321_19_01_mcor_grunt5",	PRIORITY_NORMAL )	// Blisk and his Mercs are cold blooded killers. It's a good thing someone's been picking them off one by one.
			RegisterDialogue( "Grunt_6_KillBecauseWeHaveTo", 	"diag_sp_controlBunker_BE321_20_01_mcor_grunt6",	PRIORITY_NORMAL )	// We kill because we have to. The IMC hired Blisk to kill for money. That's not noble.
			RegisterDialogue( "Grunt_11_TasteOfOwnMedicine", 	"diag_sp_controlBunker_BE321_27_01_mcor_grunt11",	PRIORITY_NORMAL )	// I heard someone's been killing off Blisk's team. Giving the mercenaries a taste of their own medicine.
			RegisterDialogue( "Grunt_12_NothingButWatch", 		"diag_sp_controlBunker_BE321_28_01_mcor_grunt12",	PRIORITY_NORMAL )	// We were just forced to watch the massacre over at the beacon. Nothing I could do but watch...
			RegisterDialogue( "Grunt_13_HeMadeItThisFar", 		"diag_sp_controlBunker_BE321_29_01_mcor_grunt13",	PRIORITY_NORMAL )	// That Pilot's gotta know some interesting tactics. He made it this far.
			RegisterDialogue( "Grunt_14_ProgrammedDrones", 		"diag_sp_controlBunker_BE321_30_01_mcor_grunt14",	PRIORITY_NORMAL )	// They didn't build this place expecting humans to hit the buttons. Nothing but programmed drones in charge down here.

			// Grunt dialogue after mission dialogue (conversation)
			RegisterDialogue( "Grunt_7_Conv1", 					"diag_sp_controlBunker_BE321_21_01_mcor_grunt7",	PRIORITY_NORMAL )	// I thought with Demeter shut down the IMC wouldn't have reinforcements.
			RegisterDialogue( "Grunt_8_Conv2", 					"diag_sp_controlBunker_BE321_22_01_mcor_grunt8",	PRIORITY_NORMAL )	// They have pleny of mindless Stalkers, that's for damn sure.
			RegisterDialogue( "Grunt_7_Conv3", 					"diag_sp_controlBunker_BE321_23_01_mcor_grunt7",	PRIORITY_NORMAL )	// I don't know if that's a good thing or a bad thing.
			RegisterDialogue( "Grunt_9_Conv4", 					"diag_sp_controlBunker_BE321_24_01_mcor_grunt9",	PRIORITY_NORMAL )	// Taking out Demeter was a huge victory for us, why doesn't it feel that way?
			RegisterDialogue( "Grunt_10_Conv5", 				"diag_sp_controlBunker_BE321_25_01_mcor_grunt10",	PRIORITY_NORMAL )	// Well, the Demeter fleet was only about a third of the IMC presence on the Frontier. Still, every little victory counts.
			RegisterDialogue( "Grunt_9_Conv6", 					"diag_sp_controlBunker_BE321_26_01_mcor_grunt9",	PRIORITY_NORMAL )	// You have the fullest half-glass I've ever seen...

			//########################################
			// Returned to Control Room with Arc Tool
			//########################################

			// Tell Pilot to enter the power relay room
			RegisterDialogue( "Grunt1_LooksLikeArcTool", 		"diag_sp_returnArcTool_BE341_01_01_mcor_grunt1", 	PRIORITY_NORMAL ) 	// The Pilot's back and that looks like an Arc Tool to me -- I think we just got our ticket off this dirt ball.
			RegisterDialogue( "BT_GoodWorkCooper", 				"diag_sp_returnArcTool_BE341_17_01_mcor_bt", 		PRIORITY_NORMAL ) 	// Good work, Pilot Cooper. Now we can jump-start the power to get the beacon functional.
			RegisterDialogue( "Grunt_CableSecure", 				"diag_sp_returnArcTool_BE341_03_01_mcor_grunt3", 	PRIORITY_NORMAL ) 	// The cable is secure, Captain.
			RegisterDialogue( "Mike_WeNeedSomePower", 			"diag_sp_returnArcTool_BE341_02_01_mcor_gcaptain", 	PRIORITY_NORMAL ) 	// Glad you made it back, sir. Just in time too, we've rewired the whole system - now all we need is some power.
			RegisterDialogue( "BT_RelaysAreOffline", 			"diag_sp_returnArcTool_BE341_08_01_mcor_bt", 		PRIORITY_NORMAL ) 	// Pilot, the power chamber relays are offline. The Arc Tool should be able to jump start them manually.
			RegisterDialogue( "Mike_WorthTheTrouble", 			"diag_sp_returnArcTool_BE341_04_01_mcor_gcaptain", 	PRIORITY_NORMAL ) 	// Pilot, let's hope that Arc Tool was worth all the trouble.

			// Nag if player doesn't enter the relay room
			RegisterDialogue( "BT_Nag_GoIntoChamber", 			"diag_sp_returnArcTool_BE341_09_01_mcor_bt", 		PRIORITY_NORMAL ) 	// The Captain will open the chamber door for you when you are ready.

			// Grunt banter while waiting for player to enter the power chamber
			RegisterDialogue( "Grunt_Banter_Wait_Power_Array1", "diag_sp_returnArcTool_BE341_05_01_mcor_bGrunt", 	PRIORITY_NORMAL ) 	// That Pilot better be careful in there...After those relays blew, they leaked radiation all over that chamber. Scorched my damn hair when I got too close.
			RegisterDialogue( "Grunt_Banter_Wait_Power_Array2", "diag_sp_returnArcTool_BE341_06_01_mcor_grunt4", 	PRIORITY_NORMAL ) 	// What hair? You're bald, Barry.
			RegisterDialogue( "Grunt_Banter_Wait_Power_Array3", "diag_sp_returnArcTool_BE341_07_01_mcor_bGrunt", 	PRIORITY_NORMAL ) 	// Not yet, still got a few survivors. Just like us.

			// Player is between inner and outer hatches of power array
			RegisterRadioDialogue( "BT_RadiationVeryHigh", 			"diag_sp_returnArcTool_BE341_10_01_mcor_bt", 		PRIORITY_NORMAL, "#NPC_BT_NAME", TEAM_MILITIA ) 	// Caution: high radiation levels detected. Your suit will only protect you for a limited time. I suggest you move quickly.
			RegisterRadioDialogue( "Mike_WouldntStayTooLong", 		"diag_sp_returnArcTool_BE341_12_01_mcor_gcaptain", 	PRIORITY_NORMAL, "#CPT_COLE", TEAM_MILITIA ) 	// I wouldn't stay in there too long unless you want to be burnt to a crisp.
			RegisterRadioDialogue( "Mike_HaveArcToolReady", 			"diag_sp_returnArcTool_BE341_13_01_mcor_gcaptain", 	PRIORITY_NORMAL, "#CPT_COLE", TEAM_MILITIA ) 	// Get that Arc Tool ready. When I open the hatch, every second will count.
			RegisterRadioDialogue( "Mike_SwitchToArcTool", 			"diag_sp_controlroom_BE140_06_01_mcor_grunt1", 		PRIORITY_NORMAL, "#CPT_COLE", TEAM_MILITIA ) 	// You need to switch to the Arc Tool, Pilot. When I open that inner hatch, every second will count.

			RegisterRadioDialogue( "Mike_OkOpeningHatch", 			"diag_sp_controlroom_BE140_07_01_mcor_grunt1", 		PRIORITY_NORMAL, "#CPT_COLE", TEAM_MILITIA ) 	// Ok, opening the hatch in 3...
			RegisterRadioDialogue( "Mike_2", 						"diag_sp_controlroom_BE140_08_01_mcor_grunt1", 		PRIORITY_NORMAL, "#CPT_COLE", TEAM_MILITIA ) 	// 2...
			RegisterRadioDialogue( "Mike_1", 						"diag_sp_controlroom_BE140_09_01_mcor_grunt1", 		PRIORITY_NORMAL, "#CPT_COLE", TEAM_MILITIA ) 	// 1...
			RegisterRadioDialogue( "Mike_Go", 						"diag_sp_controlroom_BE140_10_01_mcor_grunt1", 		PRIORITY_NORMAL, "#CPT_COLE", TEAM_MILITIA ) 	// Go!

			// Powering up the array
			RegisterRadioDialogue( "BT_100PercentPower", 			"diag_sp_returnArcTool_BE341_14_01_mcor_bt", 		PRIORITY_NORMAL, "#NPC_BT_NAME", TEAM_MILITIA ) 	// Power at 100%. I recommend you exit the chamber immediately.
			RegisterRadioDialogue( "Mike_ExitTheChamber", 			"diag_sp_returnArcTool_BE341_15_01_mcor_gcaptain", 	PRIORITY_NORMAL, "#CPT_COLE", TEAM_MILITIA ) 	// Alright Pilot, the hatch is opening. Get outta there.
			RegisterRadioDialogue( "Mike_PowerIsBack", 				"diag_sp_returnArcTool_BE341_16_01_mcor_gcaptain", 	PRIORITY_LOW, "#CPT_COLE", TEAM_MILITIA ) 	// Great job, Pilot. The power is back.

			// Control room is powered up
			RegisterDialogue( "Mike_OpenThoseShutters", 		"diag_sp_uplinkDown_BE351_16_01_mcor_gcaptain", 	PRIORITY_NORMAL ) 	// Open those blast shutters! Let's let some light in here.
			RegisterDialogue( "Grunt2_OpeningShutters", 		"diag_sp_uplinkDown_BE351_17_01_mcor_grunt4", 		PRIORITY_NO_QUEUE ) 	// Opening shutters!
			RegisterDialogue( "Facility_DiagnosticsComplete", 	"diag_sp_uplinkDown_BE351_18_01_imc_facilityPA", 	PRIORITY_NORMAL ) 	// Diagnostic complete. Power 100%.
			RegisterDialogue( "Mike_InitiateDishTargeting", 	"diag_sp_uplinkDown_BE351_19_01_mcor_gcaptain", 	PRIORITY_NORMAL ) 	// Initiate dish targeting reset.
			//RegisterDialogue( "Grunt1_InitiatingReset", 		"diag_sp_uplinkDown_BE351_20_01_mcor_grunt1", 		PRIORITY_NORMAL ) 	// Yes, sir! Initiating reset.
			RegisterDialogue( "Facility_TargetingInitiated", 	"diag_sp_uplinkDown_BE351_21_01_imc_facilityPA", 	PRIORITY_NORMAL ) 	// Uplink Targeting Module reset intiated.
			RegisterDialogue( "Grunt1_60Percent", 				"diag_sp_uplinkDown_BE351_22_01_mcor_grunt1", 		PRIORITY_NORMAL ) 	// 60%
			RegisterDialogue( "Grunt1_70Percent", 				"diag_sp_uplinkDown_BE351_23_01_mcor_grunt1", 		PRIORITY_NORMAL ) 	// 70%
			RegisterDialogue( "Grunt1_8090Percent", 			"diag_sp_uplinkDown_BE351_24_01_mcor_grunt1", 		PRIORITY_NORMAL ) 	// 80% ... 90%
			RegisterDialogue( "Facility_FaultDetected", 		"diag_sp_uplinkDown_BE351_25_01_imc_facilityPA", 	PRIORITY_NORMAL ) 	// WARNING. Fault detected. Emergency shut down.
			RegisterDialogue( "Mikes_WhatsGoingOn", 			"diag_sp_uplinkDown_BE351_26_01_mcor_gcaptain", 	PRIORITY_NORMAL ) 	// Son of a-! What's going on? Talk to me!
			RegisterDialogue( "Grunt3_NotSure", 				"diag_sp_uplinkDown_BE351_27_01_mcor_grunt3", 		PRIORITY_NORMAL ) 	// Not sure. Some sort of fail safe!
			RegisterDialogue( "BT_TargetingModuleOffline", 		"diag_sp_uplinkDown_BE351_28_01_mcor_bt", 			PRIORITY_NORMAL ) 	// Scans indicate the Uplink Targeting Module is offline.
			RegisterDialogue( "Mike_IMCOnTheirWay", 			"diag_sp_uplinkDown_BE351_29_01_mcor_gcaptain", 	PRIORITY_NORMAL ) 	// Dammit! I bet the IMC are on their way.
			RegisterDialogue( "BT_ReasonableBet", 				"diag_sp_uplinkDown_BE351_30_01_mcor_bt", 			PRIORITY_NORMAL ) 	// That is a reasonable bet.
			RegisterDialogue( "Mike_CanWeBypassModule", 		"diag_sp_uplinkDown_BE351_31_01_mcor_gcaptain", 	PRIORITY_NORMAL ) 	// McCord, can we bypass the module?
			RegisterDialogue( "Grunt2_NoSir", 					"diag_sp_uplinkDown_BE351_32_01_mcor_grunt2", 		PRIORITY_NORMAL ) 	// No, sir. Not unless we want to send a message to the entire IMC fleet.
			RegisterDialogue( "Mike_RepairedManually", 			"diag_sp_uplinkDown_BE351_33_01_mcor_gcaptain", 	PRIORITY_NORMAL ) 	// The module's gonna have to be repaired manually.
			RegisterDialogue( "BT_SituationForPilot", 			"diag_sp_uplinkDown_BE351_34_01_mcor_bt", 			PRIORITY_NORMAL ) 	// Pilot, this situation could use your skills.
			RegisterDialogue( "Mike_RepairBeaconInSite", 		"diag_sp_uplinkDown_BE351_35_01_mcor_gcaptain", 	PRIORITY_NORMAL ) 	// I hope you're up for another trip to hell, Pilot. You'll need to repair beacon on-site and get back here as fast as possible.
			RegisterDialogue( "BT_ReadyToMoveOut", 				"diag_sp_uplinkDown_BE351_36_01_mcor_bt", 			PRIORITY_NORMAL ) 	// I suggest we relocate to the beacon platform for repairs. Embark and we will move out, Pilot.

			RegisterDialogue( "BT_Nag_ReadyToMoveOut", 			"diag_sp_hubApproach_BE102_01a_01_mcor_bt", 		PRIORITY_NORMAL ) 	// Embark and we will move out Pilot.

			RegisterRadioDialogue( "Mike_BeCareful", 				"diag_sp_uplinkDown_BE351_37_01_mcor_gcaptain", 	PRIORITY_NORMAL, "#CPT_COLE", TEAM_MILITIA ) 	// Be careful. We've already lost a lot of good soldiers over there.

			//#####################
			// Crossing to Spoke 1
			//#####################

			RegisterDialogue( "BT_ScansIndicateModule", 	"diag_sp_spoke1pre_BE361_04_01_mcor_bt", 		PRIORITY_NO_QUEUE ) 	// My scans detect a functioning targeting module. 200 meters north-east.
			RegisterDialogue( "BT_ThrowIsOnlyOption", 		"diag_sp_spoke1pre_BE361_07_01_mcor_bt", 		PRIORITY_NO_QUEUE ) 	// My analysis indicates a throw is our only option here. I can throw you across the gap.
			RegisterDialogue( "BT_Nag_ClimbIntoMyHand", 	"diag_sp_finalDish_BE391_04_01_mcor_bt", 		PRIORITY_NORMAL ) 		// Standing by, Pilot. Climb onto my hand.

			RegisterDialogue( "BT_WindHeadingRange", 		"diag_sp_spoke1_BE117_10_01_mcor_bt", 			PRIORITY_NO_QUEUE ) 	// Wind: 3 knots, heading 274. Range: 372 meters. Projectile mass ----- 89 kilograms.
			RegisterDialogue( "BT_TrustMe", 				"diag_sp_spoke1_BE117_04_01_mcor_bt", 			PRIORITY_NO_QUEUE ) 	// Trust me.

			RegisterRadioDialogue( "Blisk_This_Is_Blisk", 				"diag_sp_bossComms_BE501_01_01_imc_blisk", 		PRIORITY_NORMAL, 	"#BOSSNAME_BLISK",		TEAM_IMC, true ) 	// Richter, this is Blisk.
			RegisterRadioDialogue( "Richter_Dies_Ist_Richter", 			"diag_sp_bossComms_BE501_02_01_imc_richter", 	PRIORITY_NORMAL, 	"#BOSSNAME_RICHTER",	TEAM_IMC, true ) 	// Ja, dies ist Richter.
			RegisterRadioDialogue( "Blisk_Pilot_Trying_To_Use_Beacon", 	"diag_sp_bossComms_BE501_03_01_imc_blisk", 		PRIORITY_NORMAL, 	"#BOSSNAME_BLISK",		TEAM_IMC, true ) 	// That damn Pilot is trying to use the beacon. End this. I’m counting on you. Blisk out.
			RegisterRadioDialogue( "Richter_Get_To_Beacon_Now", 		"diag_sp_bossComms_BE501_04_01_imc_richter", 	PRIORITY_NORMAL, 	"#BOSSNAME_RICHTER",	TEAM_IMC, true ) 	// All security units, this is Richter. Get to the beacon now. Find that Kleine Mann. Jetzt. Lass mich nicht hängen.
			RegisterRadioDialogue( "Comms_CopyThat", 					"diag_sp_spoke1_BE371_05_01_imc_hqcom", 		PRIORITY_NORMAL,	"#IMC_HQ_COMMS",		TEAM_IMC, true ) 	// Copy that. Zulu One, Kilo One, move in! Alpha two, get soldiers on ground! Move it!

			//################
			// Spoke 1 Combat
